#/bin/bash

echo "================================="
echo "starting upload"
cd $WORKSPACE && \
  pwd && \
  aws s3 cp ./ s3://$BUCKET_NAME \
      --recursive \
      --exclude "*" \
      --include "*.css" \
      --include "*.html" \
      --include "*.jpeg" \
      --include "*.jpg" \
      --include "*.js" \
      --include "*.png" \
      --include "*.svg" \
      --include "*.ttf" \

echo "upload complete"
echo "================================="